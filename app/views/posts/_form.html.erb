<h3> New post </h3>

<%= form_for @post do |f| %>
  <%= f.label :title, "Title " %><br>
  <%= f.text_field :title %>
  <br>
  <br>

  <%= f.label :description, "Description" %><br>
  <%= f.text_field :description %>

  <br>
  <br>

  <%= f.label :url, "Url " %> <br>
  <%= f.text_field :url %>

<br>

  <%= f.submit "Create" %>

<% end %>